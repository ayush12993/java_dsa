package com.ayushm;

public class twosums {
public int[] twoSum(int[] nums, int target) {
        int firstArrayValue = nums[0];
        for(int i=1;i<nums.length;i++) {
        	if(target-(nums[i]+firstArrayValue)==0) {
        		return new int[] {0,i};
        	}
        }
        
        return new int[] {0,1};
    }

public static void main(String[] args) {
	
}
}
